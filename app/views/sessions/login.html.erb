<h1> Login </h1>

<%= form_for @user, url:'/login' do |f| %>
<%= flash[:error] %>
<br><br>
      <%= f.label :username %>  <%= f.text_field :username %>
         <%= f.label :password %>  <%= f.password_field :password %>

         <%=f.submit "submit" %>
<% end %> 
<%= render 'layouts/homestyle' %>